<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml" stylesheets="@styles.css"
      fx:controller="org.example.carreracochesinterfaz.Controladores.ControladorCarrera">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>

    <Button fx:id="btn_iniciar" text="INICIAR" prefWidth="200" prefHeight="50" maxWidth="200" maxHeight="50"/>

    <VBox fx:id="pantalla_carrera" prefWidth="300" maxWidth="300" prefHeight="700" maxHeight="700">

        <HBox spacing="40.0" alignment="CENTER">
            <Label text="Salida"/>
            <Label text="Meta"/>


        </HBox>
        <VBox spacing="80.0" alignment="CENTER_LEFT" prefWidth="280" maxWidth="280" prefHeight="680" maxHeight="680">
            <Label styleClass="coche" text="Coche 1"/>
            <Label styleClass="coche" text="Coche 2"/>
            <Label styleClass="coche" text="Coche 3"/>
            <Label styleClass="coche" text="Coche 4"/>
            <Label styleClass="coche" text="Coche 5"/>

        </VBox>
    </VBox>


</VBox>
